{
  "version": 2,
  "buildCommand": "npm run build",
  "public": "dist/public",
  "functions": {
    "dist/index.cjs": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/index.cjs"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
